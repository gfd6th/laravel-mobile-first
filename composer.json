{
    "name": "vxm/laravel-mobile-first",
    "description": "Support implement mobile-first principle for your Laravel application.",
    "keywords": [
        "vxm",
        "laravel-mobile-first"
    ],
    "homepage": "https://github.com/vuongxuongminh/laravel-mobile-first",
    "license": "MIT",
    "authors": [
        {
            "name": "Vuong Xuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.7",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "~7.5",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "VXM\\MobileFirst\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VXM\\MobileFirst\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "\"vendor/bin/phpunit\""
    },
    "suggest": {
        "riverskies/laravel-mobile-detect": "For detect device in blade environment."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "VXM\\MobileFirst\\MobileFirstServiceProvider"
            ]
        }
	}
}
